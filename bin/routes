# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

GET     /public/*file               controllers.Assets.at(path="/public", file)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET		/search/:keywords			controllers.HomeController.search(keywords: String)

GET     /public/*file               controllers.Assets.at(path="/public", file)

GET		/getHashtags/:hashtag		controllers.HomeController.getHashtags(hashtag: String)

GET		/getLocation/:latitude/:longitude		controllers.HomeController.getLocation(latitude:String, longitude:String)

GET		/getUserProfile/:username		controllers.HomeController.getUserProfile(username:String)

GET		/tweetWords/:query			controllers.HomeController.getTweetWords(query:String)