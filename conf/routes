# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
#GET     /                           controllers.HomeController.indexGET     /                           controllers.HomeController.index

GET     /public/*file               controllers.Assets.at(path="/public", file)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#GET		/search/:keywords			controllers.HomeController.search(keywords: String)
#GET		/search/:keywords			controllers.Application.search(keywords: String)


GET     /public/*file               controllers.Assets.at(path="/public", file)

#GET		/getHashtags/:hashtag		controllers.HomeController.getHashtags(hashtag: String)

#GET		/getLocation/:latitude/:longitude		controllers.HomeController.getLocation(latitude:String, longitude:String)

#GET		/getUserProfile/:username		controllers.HomeController.getUserProfile(username:String)

#GET		/tweetWords/:query			controllers.HomeController.getTweetWords(query:String)

GET     /                           controllers.Application.index

GET		/getHashtags/:hashtag		controllers.Application.getHashtags(hashtag: String)

GET		/search/:keywords				controllers.Application.search(keywords)

GET		/getLocation/:latitude/:longitude		controllers.Application.getLocation(latitude:String, longitude:String)

GET		/getUserProfile/:username		controllers.Application.getUserProfile(username:String)

GET		/tweetWords/:query			controllers.Application.getTweetWords(query:String)